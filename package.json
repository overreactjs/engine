{
  "name": "@overreact/engine",
  "version": "1.2.0",
  "type": "module",
  "main": "./dist/engine.umd.cjs",
  "module": "./dist/engine.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/engine.js",
      "require": "./dist/engine.umd.cjs"
    },
    "./styles.css": {
      "import": "./dist/style.css",
      "require": "./dist/style.css"
    }
  },
  "files": [
    "dist"
  ],
  "description": "Browser-based game engine, built on top of React.",
  "author": "Craig Smith",
  "license": "MIT",
  "homepage": "https://overreactjs.github.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/overreactjs/engine.git"
  },
  "bugs": {
    "url": "https://github.com/overreactjs/engine/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "vitest",
    "test-ci": "vitest --coverage run"
  },
  "dependencies": {
    "detect-collisions": "^9.2.2",
    "matter-js": "^0.19.0",
    "react-merge-refs": "^3.0.2"
  },
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^14.2.1",
    "@types/matter-js": "^0.19.5",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.4.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^24.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.6.4",
    "vitest": "^1.4.0"
  }
}
